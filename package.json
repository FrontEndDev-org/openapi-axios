{
    "name": "openapi-axios",
    "version": "0.16.0",
    "description": "OpenAPI ➡️ Axios",
    "scripts": {
        "prepare": "husky install",
        "build": "vite build",
        "lint:js": "eslint --ext=.cjs,.mjs,.ts,.tsx src test",
        "lint:js:fix": "eslint --ext=.cjs,.mjs,.ts,.tsx src test --fix",
        "lint:type": " tsc --project tsconfig.json",
        "lint": "npm run lint:js && npm run lint:type",
        "lint:fix": "npm run lint:js:fix && npm run lint:type",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "engines": {
        "node": ">=14.21"
    },
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json",
        "./helpers": {
            "import": "./dist/helpers.mjs",
            "require": "./dist/helpers.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "helpers": [
                "./dist/helpers.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "bin": {
        "openapi-axios": "bin/index.cjs"
    },
    "keywords": [
        "cloudcome",
        "ydr.me",
        "OpenAPI",
        "swagger",
        "TypeScript",
        "OAS",
        "axios"
    ],
    "author": "云淡然 <hi@ydr.me> (https://ydr.me)",
    "maintainers": [
        "阿森 <zhenxs2018@foxmail.com>"
    ],
    "homepage": "https://github.com/FrontEndDev-org/openapi-axios",
    "repository": "https://github.com/FrontEndDev-org/openapi-axios",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.5",
        "chalk": "^5.3.0",
        "lodash": "^4.17.21",
        "openapi-types": "^12.1.3",
        "strict-event-emitter": "^0.5.1",
        "try-flatten": "^1.3.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.5.0",
        "@commitlint/config-conventional": "^18.5.0",
        "@commitlint/types": "^18.4.4",
        "@types/lodash": "^4.14.202",
        "@types/node": "20",
        "@types/prettier": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "@vitest/coverage-v8": "^1.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.1",
        "vite-plugin-externalize-deps": "^0.8.0",
        "vitest": "^1.2.1"
    }
}
