{
  "name": "openapi-axios",
  "version": "0.14.0",
  "description": "OpenAPI ➡️ Axios",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite build --watch",
    "build": "vite build",
    "lint:es": "eslint --ext=.cjs,.mjs,.ts,.tsx src/ test/",
    "lint:type": " tsc --project tsconfig.json",
    "lint": "npm run lint:es && npm run lint:type",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=14.21"
  },
  "sideEffects": false,
  "type": "module",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers.mjs",
      "require": "./dist/helpers.cjs",
      "types": "./dist/helpers.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "openapi-axios": "bin/index.cjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cloudcome",
    "ydr.me",
    "OpenAPI",
    "swagger",
    "TypeScript",
    "OAS",
    "axios"
  ],
  "author": "云淡然 <hi@ydr.me> (https://ydr.me)",
  "maintainers": [
    "阿森 <zhenxs2018@foxmail.com>"
  ],
  "homepage": "https://github.com/FrontEndDev-org/openapi-axios",
  "repository": "https://github.com/FrontEndDev-org/openapi-axios",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.5",
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.7",
    "strict-event-emitter": "^0.5.0",
    "try-flatten": "^1.1.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/types": "^17.4.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^14.18.42",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.18.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-externalize-deps": "^0.5.0",
    "vitest": "^0.30.1"
  }
}
