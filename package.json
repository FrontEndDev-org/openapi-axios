{
  "name": "oas-gen-ts",
  "version": "0.6.24",
  "description": "OpenAPI Specification ➡️ TypeScript",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts && tsc --project tsconfig.json --noEmit",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "build:types": "rm -rf dist-dts && tsc --project tsconfig.types.json",
    "build:files": "rm -rf dist-cjs dist-mjs && rollup --config",
    "build": "npm run build:types && npm run build:files"
  },
  "engines": {
    "node": ">=16"
  },
  "engineStrict": true,
  "type": "module",
  "module": "dist-mjs/index.mjs",
  "main": "dist-cjs/index.cjs",
  "types": "dist-dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist-mjs/index.mjs",
      "require": "./dist-cjs/index.cjs",
      "types": "./dist-dts/index.d.ts"
    },
    "./helpers": {
      "import": "./dist-mjs/helpers.mjs",
      "require": "./dist-cjs/helpers.cjs",
      "types": "./dist-dts/helpers.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "oas-gen-ts": "bin/index.cjs"
  },
  "files": [
    "dist-cjs",
    "dist-dts",
    "dist-mjs",
    "templates"
  ],
  "keywords": [
    "cloudcome",
    "ydr.me",
    "OpenAPI Specification",
    "TypeScript",
    "OAS",
    "axios"
  ],
  "author": "云淡然 <hi@ydr.me>",
  "homepage": "https://github.com/cloudcome/oas-gen-ts",
  "repository": "https://github.com/cloudcome/oas-gen-ts",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash-es": "^4.17.21",
    "swagger-typescript-api": "^12.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-c8": "^0.29.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-define-config": "^1.16.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "rollup": "^3.19.1",
    "typescript": "^5.0.2",
    "vitest": "^0.29.3"
  }
}
